<div class="worker">
	<p><%= @company.name %> </p>
  <p><%= @worker.name %></p>
  <p>Calls</p>
</div>
<div class="wtable">
	<table>
		<tr>
      <th>Received at</th>
      <th>Time left </th>
		</tr>
    <% @calls.each do |call| %>
      <tr>
        <td><%= call.created_at.localtime %></td>
        <td><%= call.time_left %></td>
      </tr>
    <% end %>
	</table>
</div>